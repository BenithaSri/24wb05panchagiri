extends layout

block content
  h1= title
  p Welcome to #{title}
  
  - let image_names = ["image1.jpg", "image2.jpg", "image3.jpg", "image4.jpg", "image5.jpg" ]
  - for (i=0;i<3;i++)
      - let index = Math.floor(Math.random()*5);
      - let displayImg = image_names[index];
      img(src=`../images/${displayImg}`, alt=`${displayImg}`, height="300", width="300")
    
        


